{
  "name": "veloz",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "node scripts/build-production.js",
    "build:dev": "node scripts/build-development.js",
    "build:data": "node scripts/build-data.js",
    "build:split-content": "node scripts/build-split-content.js",
    "test:split-content": "node scripts/test-split-content.js",
    "mark:featured": "node scripts/mark-featured-media.js",
    "clean:generated": "rm -rf src/data/content-*.json src/lib/static-content.generated.ts src/lib/build-time-data.generated.ts",
    "build:content": "node scripts/build-data.js",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "seed:about": "node scripts/seed-about-content.js",
    "migrate:slugs": "node scripts/migrate-slugs.mjs",
    "theme:check": "node scripts/theme-consistency-checker.mjs",
    "theme:accessibility": "node scripts/theme-accessibility-test.js",
    "prepare": "husky",
    "type-check": "tsc --noEmit",
    "theme:docs": "node scripts/theme-doc-generator.js",
    "theme:migrate": "node scripts/theme-migration-helper.js",
    "theme:migrate:apply": "node scripts/theme-migration-helper.js --apply",
    "theme:validate": "node scripts/theme-validation.js",
    "theme:validate:strict": "node scripts/theme-validation.js --strict",
    "theme:validate:report": "node scripts/theme-validation.js --report",
    "theme:preview": "echo 'Visit http://localhost:3000/debug/theme-preview to see theme preview'",
    "setup:env": "node scripts/setup-env.js",
    "warmup": "node scripts/warmup-pages.js",
    "warmup:prod": "SITE_URL=https://your-site.netlify.app node scripts/warmup-pages.js",
    "lighthouse": "node scripts/lighthouse-audit.js",
    "lighthouse:report": "node scripts/lighthouse-audit.js && open lighthouse-performance-report.md",
    "performance": "node scripts/performance-test.js",
    "performance:report": "node scripts/performance-test.js && open performance-report.md",
    "optimize:images": "node scripts/optimize-images.js",
    "optimize:images:report": "node scripts/optimize-images.js && echo 'Image optimization complete!'",
    "clear-cache": "node scripts/clear-cache.js",
    "clear-cache:full": "node scripts/clear-cache.js --full"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@emailjs/browser": "^4.4.1",
    "@hookform/resolvers": "^5.1.1",
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-hover-card": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toggle": "^1.1.9",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@types/qrcode": "^1.5.5",
    "beasties": "^0.3.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "embla-carousel-react": "^8.6.0",
    "firebase": "^11.9.1",
    "framer-motion": "^12.23.12",
    "glightbox": "^3.3.1",
    "isomorphic-dompurify": "^2.26.0",
    "lucide-react": "^0.522.0",
    "motion": "^12.19.1",
    "next": "15.3.4",
    "openai": "^5.8.1",
    "qrcode": "^1.5.4",
    "react": "^19.0.0",
    "react-day-picker": "^9.8.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.58.1",
    "react-is": "^18.2.0",
    "react-masonry-css": "^1.0.16",
    "recharts": "^3.1.2",
    "sonner": "^2.0.6",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@netlify/plugin-lighthouse": "^6.0.3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.4",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.0",
    "firebase-admin": "^13.4.0",
    "firebase-tools": "^14.9.0",
    "glob": "^11.0.3",
    "husky": "^9.1.7",
    "jest": "^30.0.3",
    "jest-environment-jsdom": "^30.0.2",
    "lint-staged": "^16.1.2",
    "node-fetch": "^2.7.0",
    "prettier": "^3.6.0",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.3.4",
    "typescript": "^5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "npm run theme:check"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  }
}
